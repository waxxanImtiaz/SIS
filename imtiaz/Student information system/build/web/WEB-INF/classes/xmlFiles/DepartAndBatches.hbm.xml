<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
      <class catalog="system" name="beans.DepartAndBatches" optimistic-lock="version" table="DepartAndBatches">
          <id name="batch" type="string">
         <column name="batch"/>
         <generator class="assigned"/>
        </id>
        <property name="depart" type="string">
          <column length="20" name="depart"/>
        </property>
       
    </class>
    
</hibernate-mapping>
